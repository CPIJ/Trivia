<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<GridPane maxHeight="-157" maxWidth="-387" minHeight="-157" minWidth="-387" prefHeight="157.0" prefWidth="387.0"
          xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="application.Application">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="165.0" minWidth="10.0" prefWidth="58.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="404.0" minWidth="10.0" prefWidth="309.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="48.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="95.0" minHeight="10.0" prefHeight="25.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="107.0" minHeight="10.0" prefHeight="29.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="107.0" minHeight="10.0" prefHeight="44.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="107.0" minHeight="10.0" prefHeight="34.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <padding>
        <Insets left="10.0" right="10.0"/>
    </padding>
    <Label text="Login">
        <GridPane.margin>
          <Insets/>
        </GridPane.margin>
        <font>
          <Font size="14.0"/>
        </font>
    </Label>
    <Label text="Name:" GridPane.rowIndex="1"/>
    <Label text="Password:" GridPane.rowIndex="2"/>
    <TextField id="txt_password" fx:id="txt_password" prefHeight="25.0" prefWidth="296.0" GridPane.columnIndex="1"
               GridPane.rowIndex="2"/>
    <TextField id="txt_name" fx:id="txt_name" prefHeight="25.0" prefWidth="296.0" GridPane.columnIndex="1"
               GridPane.rowIndex="1"/>
    <Button id="btn_login" fx:id="btn_login" mnemonicParsing="false" onAction="#btnClicked" prefHeight="25.0"
            prefWidth="59.0" text="Login" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
    <Button mnemonicParsing="false" text="Register" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
</GridPane>
